<AnchorPane prefHeight="893.0" prefWidth="1207.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ShowOrderDetailController">
   <children>
      <TableView fx:id="tableView" layoutX="52.0" layoutY="108.0" prefHeight="576.0" prefWidth="680.0">
        <columns>
          <TableColumn fx:id="ID" prefWidth="146.0" text="ProductID" />
            <TableColumn fx:id="quantity" minWidth="0.0" prefWidth="176.0" text="Amount" />
            <TableColumn fx:id="price" prefWidth="140.0" text="Price" />
          <TableColumn fx:id="name" minWidth="0.0" prefWidth="216.0" text="Tel" />
        </columns>
      </TableView>
      <Button fx:id="checkstock" layoutX="805.0" layoutY="495.0" mnemonicParsing="false" onAction="#handlecheckstock" prefHeight="71.0" prefWidth="102.0" text="Checkstock" />
      <Label fx:id="label" layoutX="783.0" layoutY="100.0" prefHeight="276.0" prefWidth="364.0" />
      <Button fx:id="backBtn" layoutX="872.0" layoutY="613.0" mnemonicParsing="false" onAction="#handleBackBtn" prefHeight="71.0" prefWidth="102.0" text="Back" />
      <Label layoutX="282.0" layoutY="40.0" text="OrderDetail">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <Button fx:id="subBtn" disable="true" layoutX="965.0" layoutY="495.0" mnemonicParsing="false" onAction="#handleSubBtn" prefHeight="71.0" prefWidth="121.0" text="Summit" />
       </children>
</AnchorPane>
